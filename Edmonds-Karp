#include <iostream>
#include <vector>

using namespace std;

const int inf=100000;

int main() {
	int v, e;
	cin>>v>>e;
	vector <vector <int>> q (v);
	for (int i=0; i<v; i++) {
		for (int j=0; j<v; j++) {
			cin>>q[i][j];
		}
	}
	vector <vector <int>> f (v);
	for (;;) {
		vector <int> from (v, -1);
		vector <int> g (v);
		int h=0;
		int t=0;
		g[t++]=0;
		from[0]=0;
		for (int n=0; h<t;) {
			n=g[h++];
			for (int j=0; j<v; j++) {
				if (from[j]==-1 && q[n][j]-f[n][j]>0) {
					g[t++]=j;
					from[j]=n;
				}
			}
		}
		if (from[v-1]==-1) {
			break;
		}
		int c=inf;
		for (int n=v-1; n!=0;) {
			int prev=from[n];
			if (c>q[prev][n]-f[prev][n]) {
			    c=q[prev][n]-f[prev][n];
			}
			n=prev;
		}
		for (int n=v-1; n!=0;) {
			int prev=from[n];
			f[prev][n]+=c;
			f[n][prev]-=c;
			n=prev;
		}
	}
	int flow=0;
	for (int i=0; i<v; i++) {
		if (q[0][i]) {
			flow+=f[0][i];
		}
	}
	cout<<flow;
	return 0;
}
