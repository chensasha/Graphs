#include <iostream>

using namespace std;

void matrix1 (int **p, int **arr, int v, int e) {
    for (int i=0; i<v; i++) { 
        for (int j=0; j<v; j++) {
            p[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        p[arr[i][0]-1][arr[i][1]-1]=1;
        p[arr[i][1]-1][arr[i][0]-1]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<v; j++) {
            cout<<p[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl;
}
void matrix2 (int **q, int **arr, int v, int e) {
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            q[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        q[arr[i][0]-1][i]=1;
        q[arr[i][1]-1][i]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            cout<<q[i][j]<<" ";
        }
        cout<<endl;
    } 
    cout<<endl;
}

void list(int v, int **arr, vector<vector<int> >& l) {
    for(int i=0; i<v; i++) {
        l[arr[i][0]].push_back(arr[i][1]);
        l[arr[i][1]].push_back(arr[i][0]);
    }
}

int main()
{
    int e,v; //количество ребер, вершин
    cin>>e>>v;
    int **arr=new int* [e];
    for (int i=0; i<e; i++) {
        arr[i]=new int [2];
    }
    for (int i=0; i<e; i++) {
        cin>>arr[i][0]>>arr[i][1];
    }
    int **p=new int* [v]; //матрица смежности 
    for (int i=0; i<v; i++) {
        p[i]=new int [v];
    }
    matrix1 (p, arr, v, e);
    int **q=new int* [v]; //матрица инциденций 
    for (int i=0; i<v; i++) {
        q[i]=new int [e];
    }
    matrix2 (q, arr, v, e);
    vector <vector <int>> l;
    list (v, arr, l);
    return 0;
}
