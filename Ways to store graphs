#include <iostream>

using namespace std;

void matrix1 (int **p, int **arr, int v, int e) {
    for (int i=0; i<v; i++) { 
        for (int j=0; j<v; j++) {
            p[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        p[arr[i][0]-1][arr[i][1]-1]=1;
        p[arr[i][1]-1][arr[i][0]-1]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<v; j++) {
            cout<<p[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl;
}
void matrix2 (int **q, int **arr, int v, int e) {
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            q[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        q[arr[i][0]-1][i]=1;
        q[arr[i][1]-1][i]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            cout<<q[i][j]<<" ";
        }
        cout<<endl;
    } 
    cout<<endl;
}

void list (int **l, int **p, int v, int max) {
    int k=0;
    int j=0;
    for (int i=0; i<v; i++) {
        while (k<v) {
            if (p[i][k]==1) {
                l[i][j]=k+1;
                j++;
            }
            k++;
        }
        j=0;
        k=0;
    }
    for (int i=0; i<v; i++) {
        cout<<i+1<<":"<<" ";
        for (int w=0; w<max; w++) {
            if (l[i][w]!=0) {
                cout<<l[i][w]<<" ";
            }
        }
        cout<<endl;
    }
}

int main()
{
    int e,v; //количество ребер, вершин
    cin>>e>>v;
    int **arr=new int* [e];
    for (int i=0; i<e; i++) {
        arr[i]=new int [2];
    }
    for (int i=0; i<e; i++) {
        cin>>arr[i][0]>>arr[i][1];
    }
    int **p=new int* [v]; //матрица смежности 
    for (int i=0; i<v; i++) {
        p[i]=new int [v];
    }
    matrix1 (p, arr, v, e);
    int **q=new int* [v]; //матрица инциденций 
    for (int i=0; i<v; i++) {
        q[i]=new int [e];
    }
    matrix2 (q, arr, v, e);
    int max=0;
    int s=0;
    for (int i=0; i<v; i++) {
        for (int j=0; j<v; j++) {
            s+=p[i][j];
        }
        if (max<s) {
            max=s;
        }
        s=0;
    }
    int **l=new int* [v]; //список смежностей 
    for (int i=0; i<v; i++) {
        l[i]=new int [max];
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<max; j++) {
            l[i][j]=0;
        }
    }
    list (l, p, v, max);
    return 0;
}
