#include <iostream>
#include <vector>

using namespace std;

const int inf=100000;

void list(int v, int **arr, vector<vector<int> >& l) {
    for(int i=0; i<v; i++) {
        l[arr[i][0]].push_back(arr[i][1]);
        l[arr[i][1]].push_back(arr[i][0]);
    }
}

int main() {
	int v, e;
	cin>>v>>e;
	int **arr=new int* [e];
    int *cost=new int [e];
    for (int i=0; i<e; i++) {
        arr[i]=new int [2];
    }
    for (int i=0; i<e; i++) {
        cin>>arr[i][0]>>arr[i][1];
        cin>>cost[i];
    }
	vector <vector <int>> q;
	list (v, arr, q);
	vector <vector <int>> f (v);
	for (;;) {
		vector <int> from (v, -1);
		vector <int> g (v);
		int h=0;
		int t=0;
		g[t++]=0;
		from[0]=0;
		for (int n=0; h<t;) {
			n=g[h++];
			for (int j=0; j<v; j++) {
				if (from[j]==-1 && q[n][j]-f[n][j]>0) {
					g[t++]=j;
					from[j]=n;
				}
			}
		}
		if (from[v-1]==-1) {
			break;
		}
		int c=inf;
		for (int n=v-1; n!=0;) {
			int prev=from[n];
			if (c>q[prev][n]-f[prev][n]) {
			    c=q[prev][n]-f[prev][n];
			}
			n=prev;
		}
		for (int n=v-1; n!=0;) {
			int prev=from[n];
			f[prev][n]+=c;
			f[n][prev]-=c;
			n=prev;
		}
	}
	int flow=0;
	for (int i=0; i<v; i++) {
		if (q[0][i]) {
			flow+=f[0][i];
		}
	}
	cout<<flow;
	return 0;
}
