#include <iostream>

using namespace std;

int main()
{
    int e,v; //количество ребер, вершин
    cin>>e>>v;
    int arr[e][2];
    for (int i=0; i<e; i++) {
        cin>>arr[i][0]>>arr[i][1];
    }
    int p[v][v]; //матрица смежности 
    for (int i=0; i<v; i++) { 
        for (int j=0; j<v; j++) {
            p[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        p[arr[i][0]-1][arr[i][1]-1]=1;
        p[arr[i][1]-1][arr[i][0]-1]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<v; j++) {
            cout<<p[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl;
    int q[v][e]; //матрица инциденций 
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            q[i][j]=0;
        }
    }
    for (int i=0; i<e; i++) {
        q[arr[i][0]-1][i]=1;
        q[arr[i][1]-1][i]=1;
    }
    for (int i=0; i<v; i++) {
        for (int j=0; j<e; j++) {
            cout<<q[i][j]<<" ";
        }
        cout<<endl;
    } 
    cout<<endl;
    for (int i=0; i<v; i++) { //список смежности
        cout<<i+1<<":"<<" ";
        for (int j=0; j<v; j++) {
            if (p[i][j]==1) {
                cout<<j+1<<" ";
            }
        }
        cout<<endl;
    }
    return 0;
}
